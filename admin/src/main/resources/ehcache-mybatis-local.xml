<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ehcache.sf.net/ehcache.xsd" updateCheck="false">

	<diskStore path="java.io.tmpdir/mybatis/znrl" />

	<!-- DefaultCache setting. -->
	<defaultCache 
		maxEntriesLocalHeap="10000" 
		eternal="false"
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600" 
		overflowToDisk="true"
		maxEntriesLocalDisk="100000" 
		memoryStoreEvictionPolicy="LFU"/>
		
	<!-- security entity-->
	<cache name="org.gxz.znrl.entity.User"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true" 
		maxEntriesLocalDisk="10000" />	
	<cache 
		name="org.gxz.znrl.entity.UserRole"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />		
	<cache 
		name="org.gxz.znrl.entity.RolePermission"
		maxEntriesLocalHeap="10000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="1000000" />
	<cache 
		name="org.gxz.znrl.entity.Role"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />
	<cache 
		name="org.gxz.znrl.entity.Permission"
		maxEntriesLocalHeap="10000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="100000" />
	<cache 
		name="org.gxz.znrl.entity.OrganizationRole"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />
	<cache 
		name="org.gxz.znrl.entity.Organization"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />	
	<cache 
		name="org.gxz.znrl.entity.Module"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />
	<cache 
		name="org.gxz.znrl.controller.DataControl"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />	
	<cache 
		name="org.gxz.znrl.entity.RolePermissionDataControl"
		maxEntriesLocalHeap="1000" 
		eternal="false" 
		overflowToDisk="true"
		maxEntriesLocalDisk="10000" />																	
</ehcache>
